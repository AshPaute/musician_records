-- Create the database
CREATE DATABASE musician_db;

-- Use the database
USE musician_db;

-- Create the 'musicians' table
CREATE TABLE musicians (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(100),
    instrument VARCHAR(50),
    genre VARCHAR(50),
    debut_year INT
);

-- Insert sample records into 'musicians' table
INSERT INTO musicians (name, instrument, genre, debut_year) VALUES
('John Doe', 'Guitar', 'Rock', 2005),
('Jane Smith', 'Piano', 'Classical', 2010),
('Mike Johnson', 'Drums', 'Jazz', 2000);
